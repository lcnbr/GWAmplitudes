@incollection{Abdikamalov:2020jzn,
  title = {Gravitational {{Waves}} from {{Core-Collapse Supernovae}}},
  booktitle = {Handbook of {{Gravitational Wave Astronomy}}},
  author = {Abdikamalov, Ernazar and Pagliaroli, Giulia and Radice, David},
  editor = {Bambi, Cosimo and Katsanevas, Stavros and Kokkotas, Konstantinos D.},
  year = {2020},
  eprint = {2010.04356},
  eprinttype = {arxiv},
  primaryclass = {astro-ph.SR},
  pages = {1--37},
  publisher = {{Springer}},
  address = {{Singapore}},
  doi = {10.1007/978-981-15-4702-7_21-1},
  abstract = {We summarize our current understanding of gravitational wave emission from core-collapse supernovae. We review the established results from multi-dimensional simulations and, wherever possible, provide back-of-the-envelope calculations to highlight the underlying physical principles. The gravitational waves are predominantly emitted by protoneutron star oscillations. In slowly rotating cases, which represent the most common type of the supernovae, the oscillations are excited by multi-dimensional hydrodynamic instabilities, while in rare rapidly rotating cases, the protoneutron star is born with an oblate deformation due to the centrifugal force. The gravitational wave signal may be marginally visible with current detectors for a source within our galaxy, while future third-generation instruments will enable more robust and detailed observations. The rapidly rotating models that develop non-axisymmetric instabilities may be visible up to a megaparsec distance with the third-generation detectors. Finally, we discuss strategies for multi-messenger observations of supernovae.},
  archiveprefix = {arXiv},
  isbn = {9789811547027},
  langid = {english},
  keywords = {Convection,Core-collapse supernovae,Gravitational waves,Multi-messenger,Neutrinos,Oscillations,Protoneutron star,SASI,Shock},
  annotation = {28 citations (INSPIRE 2022/9/30) 24 citations w/o self (INSPIRE 2022/9/30)},
  file = {C\:\\Users\\Lucien\\Zotero\\storage\\2YB9TXHH\\Abdikamalov et al_2020_Gravitational Waves from Core-Collapse Supernovae.pdf}
}

@article{Abreu:2017,
  title = {Cuts from Residues: The One-Loop Case},
  shorttitle = {Cuts from Residues},
  author = {Abreu, Samuel and Britto, Ruth and Duhr, Claude and Gardi, Einan},
  year = {2017},
  month = jun,
  journal = {Journal of High Energy Physics},
  volume = {2017},
  number = {6},
  eprint = {1702.03163},
  eprinttype = {arxiv},
  pages = {114},
  issn = {1029-8479},
  doi = {10.1007/JHEP06(2017)114},
  abstract = {Using the multivariate residue calculus of Leray, we give a precise definition of the notion of a cut Feynman integral in dimensional regularization, as a residue evaluated on the variety where some of the propagators are put on shell. These are naturally associated to Landau singularities of the first type. Focusing on the one-loop case, we give an explicit parametrization to compute such cut integrals, with which we study some of their properties and list explicit results for maximal and next-to-maximal cuts. By analyzing homology groups, we show that cut integrals associated to Landau singularities of the second type are specific combinations of the usual cut integrals, and we obtain linear relations among different cuts of the same integral. We also show that all one-loop Feynman integrals and their cuts belong to the same class of functions, which can be written as parametric integrals.},
  archiveprefix = {arXiv},
  keywords = {High Energy Physics - Phenomenology,High Energy Physics - Theory},
  file = {C\:\\Users\\Lucien\\Zotero\\storage\\46HQ5C26\\Abreu et al_2017_Cuts from residues.pdf;C\:\\Users\\Lucien\\Zotero\\storage\\DWFN9JGH\\Abreu et al_2017_Cuts from residues.html}
}

@article{AccettulliHuber:2020dal,
  title = {From Amplitudes to Gravitational Radiation with Cubic Interactions and Tidal Effects},
  author = {Huber, Manuel Accettulli and Brandhuber, Andreas and Angelis, Stefano De and Travaglini, Gabriele},
  year = {2021},
  month = feb,
  journal = {Physical Review D},
  volume = {103},
  number = {4},
  eprint = {2012.06548},
  eprinttype = {arxiv},
  primaryclass = {hep-th},
  pages = {045015},
  doi = {10.1103/PhysRevD.103.045015},
  abstract = {We study the effect of cubic and tidal interactions on the spectrum of gravitational waves emitted in the inspiral phase of the merger of two non-spinning objects. There are two independent parity-even cubic interaction terms, which we take to be \$I\_1 = \{R\^\{\textbackslash alpha \textbackslash beta\}\}\_\{\textbackslash mu \textbackslash nu\} \{R\^\{\textbackslash mu \textbackslash nu\}\}\_\{\textbackslash rho \textbackslash sigma\} \{R\^\{\textbackslash rho \textbackslash sigma\}\}\_\{\textbackslash alpha \textbackslash beta\}\$ and \$G\_3 = I\_1-2 R\^\{\textbackslash alpha\}\textbackslash,\_\{\textbackslash mu\}\textbackslash,\^\{\textbackslash beta\}\textbackslash,\_\{\textbackslash nu\} R\^\{\textbackslash mu\}\textbackslash,\_\{\textbackslash rho\}\textbackslash,\^\{\textbackslash nu\}\textbackslash,\_\{\textbackslash sigma\} R\^\{\textbackslash rho\}\textbackslash,\_\{\textbackslash alpha\}\textbackslash,\^\{\textbackslash sigma\}\textbackslash,\_\{\textbackslash beta\}\$. The latter has vanishing pure graviton amplitudes but modifies mixed scalar/graviton amplitudes which are crucial for our study. Working in an effective field theory set-up, we compute the modifications to the quadrupole moment due to \$I\_1\$, \$G\_3\$ and tidal interactions, from which we obtain the power of gravitational waves radiated in the process to first order in the perturbations and leading order in the post-Minkowskian expansion. The \$I\_1\$ predictions are novel, and we find that our results for \$G\_3\$ are related to the known quadrupole corrections arising from tidal perturbations, although the physical origin of the \$G\_3\$ coupling is unrelated to the finite-size effects underlying tidal interactions. We show this by recomputing such tidal corrections and by presenting an explicit field redefinition. In the post-Newtonian expansion our results are complete at leading order, which for the gravitational-wave flux is 5PN for \$G\_3\$ and tidal interactions, and 6PN for \$I\_1\$. Finally, we compute the corresponding modifications to the waveforms.},
  archiveprefix = {arXiv},
  arxivid = {2012.06548},
  annotation = {43 citations (INSPIRE 2022/9/27) 41 citations w/o self (INSPIRE 2022/9/27)}
}

@article{AccettulliHuber:2020oou,
  title = {Eikonal Phase Matrix, Deflection Angle, and Time Delay in Effective Field Theories of Gravity},
  author = {Huber, Manuel Accettulli and Brandhuber, A. and Angelis, S. De and Travaglini, G.},
  year = {2020},
  month = aug,
  journal = {Physical Review D},
  volume = {102},
  number = {4},
  eprint = {2006.02375},
  eprinttype = {arxiv},
  primaryclass = {hep-th},
  pages = {046014},
  doi = {10.1103/PhysRevD.102.046014},
  abstract = {The eikonal approximation is an ideal tool to extract classical observables in gauge theory and gravity directly from scattering amplitudes. Here we consider effective theories of gravity where in addition to the Einstein-Hilbert term we include non-minimal couplings of the type \$R\^3\$, \$R\^4\$ and \$FFR\$. In particular, we study the scattering of gravitons and photons of frequency \$\textbackslash omega\$ off heavy scalars of mass \$m\$ in the limit \$m\textbackslash gg \textbackslash omega \textbackslash gg |\textbackslash vec\{q\}\textbackslash,|\$, where \$\textbackslash vec\{q\}\$ is the momentum transfer. The presence of non-minimal couplings induces helicity-flip processes which survive the eikonal limit, thereby promoting the eikonal phase to an eikonal phase matrix. We obtain the latter from the relevant two-to-two helicity amplitudes that we compute up to one-loop order, and confirm that the leading-order terms in \$\textbackslash omega\$ exponentiate \textbackslash `\{a\} la Amati, Ciafaloni and Veneziano. From the eigenvalues of the eikonal phase matrix we then extract two physical observables, to 2PM order: the classical deflection angle and Shapiro time delay/advance. Whenever the classical expectation of helicity conservation of the massless scattered particle is violated, i.e. the eigenvalues of the eikonal matrix are non-degenerate, causality violation due to time advance is a generic possibility for small impact parameter. We show that for graviton scattering in the \$R\^4\$ and \$FFR\$ theories, time advance is circumvented if the couplings of these interactions satisfy certain positivity conditions, while it is unavoidable for graviton scattering in the \$R\^3\$ theory and photon scattering in the \$FFR\$ theory. The scattering processes we consider mimic the deflection of photons and gravitons off spinless heavy objects such as black\textasciitilde holes.},
  archiveprefix = {arXiv},
  arxivid = {2006.02375},
  annotation = {62 citations (INSPIRE 2022/9/27) 57 citations w/o self (INSPIRE 2022/9/27)}
}

