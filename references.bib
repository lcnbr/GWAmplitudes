@article{Cristofoli:2020uzm,
  title = {Second-Order Post-{{Minkowskian}} Scattering in Arbitrary Dimensions},
  author = {Cristofoli, A. and Damgaard, P. and Vecchia, Paolo Di and Heissenberg, C.},
  year = {2020},
